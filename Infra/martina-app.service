[Unit]
Description=Martina .NET Web App
After=network.target

[Service]
WorkingDirectory=/var/www/martina-app
ExecStart=/usr/bin/dotnet /var/www/martina-app/Exam1.dll
Restart=always
RestartSec=10
SyslogIdentifier=martina-app
User=www-data
Environment=ASPNETCORE_ENVIRONMENT=Production

[Install]
WantedBy=multi-user.target